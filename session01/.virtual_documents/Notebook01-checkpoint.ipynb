ax = fig.add_subplot(111, projection='3d')
x = [0, 0,1,1]
y = [0, 1, 1, -4/11]
z1 = [2.0, 1.3333333333333335, 1.0, 1.9090909090909092]

vertices = [list(zip(x,y,z1))]
